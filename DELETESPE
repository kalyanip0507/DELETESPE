#include<stdio.h>
int main()
{
    int new,pos,i,a[100],n;
    printf("enter size");
    scanf("%d",&n);
    printf("enter elements");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    printf("enter position");
    scanf("%d",&pos);
    for(i=pos-1;i<n-1;i++)
    {
a[i]=a[i+1];
    }
for(i=0;i<n-1;i++){
printf("%d",a[i]);
}
return 0;
}
